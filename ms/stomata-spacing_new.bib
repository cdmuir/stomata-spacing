	keywords = {BIOLOGICAL assay, Environmental stress, EPIDERMIS, GEOGRAPHICAL perception, One cell spacing rule, SALT, SPATIAL ecology, STOMATA, Stomatal clustering, Stomatal development, Vicia faba L.},
	pages = {325--336},
}

